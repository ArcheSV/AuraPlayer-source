{
  "name": "aura-player",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist-electron/main.js",
  "author": "ArcheSV",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArcheSV/AuraPlayer-source.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slider": "^1.3.5",
    "electron-updater": "^6.3.9",
    "firebase": "^10.14.1",
    "lucide-react": "^0.303.0",
    "play-dl": "^1.9.7",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-player": "^2.14.1",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@mightymeld/runtime": "^0.1.51",
    "@types/react": "^18.2.43",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "build": {
    "appId": "com.archesv.auraplayer",
    "productName": "AuraPlayer",
    "publish": [
      {
        "provider": "github",
        "owner": "ArcheSV",
        "repo": "AuraPlayer-source"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "bin",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}